<section id="filter">
<div id="panel">	

<h3>Current Users</h3>

<table class="admin_table">
<tr>
<th>Name</th>
<th>Email</th>
</tr>
<% @users.each do |user| %>
<tr class="<%= cycle("even", "odd") %>">
<td><%= link_to user.name, edit_admin_user_path(user) %></td>
<td><%= user.email %></td>
</tr>
<% end -%>
</table>

<%= will_paginate @users %>

</div>
</section>
