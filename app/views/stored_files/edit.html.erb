<%= form_for @stored_file, :html => { :id => "edit_stored_file" } do |f| %>
	<%= flash[:error] %>
	<%= f.label :original_filename %>: <%= @stored_file.original_filename %><br />
	<%= f.label :contributor %>: <%= @stored_file.author %><br />
	<%= f.label :file_size %>: <%= @stored_file.file_size %><br />
	URL: <%= stored_file_url %><br />
	<% if @stored_file.batch_id -%>
	<%= f.label "Batch" %>: <%= @stored_file.batch_id %><br />
	<% end -%>
	<%= f.label :ingest_date %>: <%= @stored_file.created_at %><br />
	<% if !@stored_file.deletion_date.nil? %>
		<%= f.label :deletion_date %>: <%= @stored_file.deletion_date %><br />
	<% end %>
	<%= @record %><br />
	<%= render :partial => "form", :locals => { :f => f } %>
<% end %>
