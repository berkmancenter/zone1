<% if attrs.include?(field) %>
	<% if type == :check_box %>
		<%= bulk_edit field %>
		<%= form.send(type, field, options) %>
		<%= form.label label %>
	<% else -%>
		<%= bulk_edit field %>
		<%= form.label "#{label}:" %>
		<%= form.send(type, field, options) %>
	<% end -%>
<% else -%>
	<%= form.label "#{label}:" %>
	<%= @stored_file.send(field) %><br />
<% end -%>
