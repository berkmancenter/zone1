<% if @facets -%>
	<% if @facets[:license_id] -%>
		<% @facets[:license_id].each do |license| %>
		<%= link_to license[:label], license[:url] %>
		<% end -%>
	<% end -%>
<% else -%>
	<% License.all.each do |license| %>
	<%= link_to license.name, url_for(:controller => "search", :action => "index", :license_id => license.id) %>
	<% end -%>
<% end -%>
